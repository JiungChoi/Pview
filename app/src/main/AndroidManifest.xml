<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jiung.fastcampus.aop.part2.pview">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.FLASHLIGHT"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_pview"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_pview"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pview"
        android:name=".PictureInit">
        <activity android:name=".StartAppActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--<activity android:name=".ReferenceActivity"/>-->
        <activity android:name=".PersonalActivity"/>
        <activity android:name=".PictureActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".ChartActivity"/>

    </application>

</manifest>